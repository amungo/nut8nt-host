&sdhci0 {
	status = "okay";
	xlnx,has-power = <0x0>;
	no-1-8-v;
	disable-wp;
};

/* ETH PHY */
 
&gem3 {
    phy-handle = <&phy0>;
    phy0: phy@c {
	reg = <0xd>;
	device_type= "ethernet-phy";
	ti,rx-internal-delay = <0x8>;
	ti,tx-internal-delay = <0xa>;
	ti,fifo-depth = <0x1>;
	ti,rxctrl-strap-worka;
    };
};

&usb0 {
	status = "okay";
	xlnx,usb-polarity = <0x0>;
	xlnx,usb-reset-io = <0x4c>;
	xlnx,usb-reset-mode = <0x2>;
};


&dwc3_0{
    dr_mode = "peripheral";
    status = "okay";
   
    snps,dis_u2_susphy_quirk;
    snps,dis_u3_susphy_quirk;
    maximum-speed = "super-speed";

    phy-names = "usb3-phy";
    phys = <&lane2 0x4 0x0 0x0 26000000>;
};

&spi0 {
  is-decoded-cs = <0>;
  num-cs = <3>;
  status = "okay";
  spidev@0x00 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <0>;
  };
  spidev@0x01 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <1>;
  };
  spidev@0x02 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <2>;
  };
};

&spi1 {
  is-decoded-cs = <0>;
  num-cs = <3>;
  status = "okay";
  spidev@0x00 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <0>;
  };
  spidev@0x01 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <1>;
  };
  spidev@0x02 {
    compatible = "spidev";
    spi-max-frequency = <1000000>;
    reg = <2>;
  };
};


